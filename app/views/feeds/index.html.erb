<h1>feed</h1>
<% @feeds.each do |feed| %>
<table>
<tr>
<td><%= feed.title %></td>
<td><%= feed.content %></td>
<td><%= User.find_by(id: feed.user_id).name %>
<td><%= link_to '詳細',feed_path(feed.id) %></td>
<td><%= link_to '編集する',edit_feed_path(feed.id)  %></td>
<td><%= link_to '削除する',feed_path(feed.id) ,method: :delete %></td>
</tr>
</table>
<% end %>
